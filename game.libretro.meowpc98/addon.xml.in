<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.meowpc98"
		name="NEC - PC-98 (Neko Project II)"
		version="0.86.0.5"
		provider-name="Neko Project II Team">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms></platforms>
		<extensions>d98|zip|98d|fdi|fdd|2hd|tfd|d88|88d|hdm|xdf|dup|cmd|hdi|thd|nhd|hdd</extensions>
		<supports_vfs>false</supports_vfs>
		<supports_standalone>false</supports_standalone>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en_GB">NEC - PC-98 (Neko Project II) 0.86+</summary>
		<description lang="en_GB">Neko Project II is an excellent PC-98 emulator.

Supported files: .d98, .zip, .98d, .fdi, .fdd, .2hd, .tfd, .d88, .88d, .hdm, .xdf, .dup, .cmd, .hdi, .thd, .nhd, .hdd</description>
		<license>MIT</license>
		<platform>@PLATFORM@</platform>
		<source>https://github.com/kodi-game/game.libretro.meowpc98</source>
	</extension>
</addon>
